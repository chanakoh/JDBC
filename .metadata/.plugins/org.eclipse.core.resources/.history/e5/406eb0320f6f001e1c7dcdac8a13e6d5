package com.kh.dtoSample;

public class ProductsDTO {
private int productid;
private String productname;
private String category;
private Double price;
private int stockqyantity;

public ProductsDTO() {}   //기본생성자

public int getProductid() { // iv대신 자정된 값을 가져오기 위한 메서드
	return productid;
}

public void setProductid(int productid) { // iv대신 값을 저장하기 위한 메서드
	this.productid = productid;
}

public String getProductname() {
	return productname;
}

public void setProductname(String productname) {
	this.productname = productname;
}

public String getCategory() {
	return category;
}

public void setCategory(String category) {
	this.category = category;
}

public Double getPrice() {
	return price;
}

public void setPrice(Double price) {
	this.price = price;
}

public int getStockqyantity() {
	return stockqyantity;
}

public void setStockqyantity(int stockqyantity) {
	this.stockqyantity = stockqyantity;
}
@Override
public String toString() { // 값을 String으로 만들어줌 근데 왜 사용?
	return "ProductsDTO";
}

}
